      write(*,*)'This plot has been saved as'
      write(*,*)'\'myinterpe.ps\'.'
      i = ifeffit('plot(device="/ps",file="myinterpe.ps")')
      call print_echo

c -----------------------------------------------------------------
c     END MY INTERPOLATION GRAPHS
